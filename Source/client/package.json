{
  "name": "grpc-node-client",
  "version": "1.0.0",
  "description": "NodeJS Server to host the Flutter gRPC client",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "npm run generate:proto && node server.js",
    "generate:proto": "npx protoc --ts_proto_out=protoDefinitions --proto_path=../proto ../proto/*.proto"
  },
  "keywords": [
    "gRPC",
    "NodeJS",
    "Typescript",
    "server"
  ],
  "author": "Charlie Hahm",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.13.3",
    "@grpc/proto-loader": "^0.7.15",
    "express": "^5.1.0",
    "http": "^0.0.1-security",
    "https": "^1.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.4",
    "@types/node": "^22.14.1",
    "ts-proto": "^2.11.2"
  }
}
