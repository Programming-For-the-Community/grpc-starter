{
  "name": "grpc-node-server",
  "version": "1.0.0",
  "description": "NodeJS gRPC server",
  "main": "server.ts",
  "type": "commonjs",
  "scripts": {
    "build": "npm run lint && tsc",
    "start": "npm run build && node dist/server.js",
    "dev": "npm run format && npm run lint:fix && tsnd --respawn server.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write **/*.{js,ts}",
    "generate:proto": "npx protoc --ts_proto_out=protoDefinitions --proto_path=../proto ../proto/*.proto"
  },
  "keywords": [
    "gRPC",
    "NodeJS",
    "Typescript",
    "server"
  ],
  "author": "Charlie Hahm",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.788.0",
    "@aws-sdk/lib-dynamodb": "^3.789.0",
    "@grpc/grpc-js": "^1.13.3",
    "@grpc/proto-loader": "^0.7.15",
    "aws-sdk": "^2.1692.0",
    "dotenv": "^16.5.0",
    "google-proto-files": "^4.2.0",
    "grpc-node-server": "file:",
    "log4js": "^6.9.1",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "grpc-tools": "^1.13.0",
    "prettier": "^3.5.3",
    "protoc": "^1.1.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "ts-proto": "^2.7.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0"
  }
}
